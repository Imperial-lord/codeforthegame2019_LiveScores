        <activity android:name="com.news.abhishek.newsapp.About_app"
            android:configChanges="orientation|screenSize"/>
        <activity android:name="com.news.abhishek.newsapp.ScoreDisplay"
            android:configChanges="orientation|screenSize"/>
        <activity
            android:name="com.news.abhishek.newsapp.Question5"
            android:label="Question5"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.news.abhishek.newsapp.Question4"
            android:label="Question4"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.news.abhishek.newsapp.Question3"
            android:label="Question3"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.news.abhishek.newsapp.Question2"
